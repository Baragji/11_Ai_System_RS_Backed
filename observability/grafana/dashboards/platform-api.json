{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "datasource",
          "uid": "grafana"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "timeseries",
      "title": "Request Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {},
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"platform-api\"}[1m]))",
          "legendFormat": "Requests/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "P95 Latency",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "job:request_duration_seconds:95p * 1000",
          "legendFormat": "p95"
        },
        {
          "expr": "job:request_duration_seconds:99p * 1000",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Redis Availability",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "avg(redis_up)",
          "legendFormat": "redis_up"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Kafka Consumer Lag",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 18,
        "x": 6,
        "y": 8
      },
      "targets": [
        {
          "expr": "max(kafka_consumergroup_lag{consumergroup=\"planner-agents\"})",
          "legendFormat": "planner"
        },
        {
          "expr": "max(kafka_consumergroup_lag{consumergroup=\"coder-agents\"})",
          "legendFormat": "coder"
        },
        {
          "expr": "max(kafka_consumergroup_lag{consumergroup=\"critic-agents\"})",
          "legendFormat": "critic"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["platform-api"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
